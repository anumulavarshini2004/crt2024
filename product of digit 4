#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    int product=1;
    scanf("%d",&n);
    while(n>0)
    {
        int digit=n % 10;
        product *=digit;
        n /=10;
    }
    printf("%d",product);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
