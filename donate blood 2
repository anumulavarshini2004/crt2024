#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int age,weight,hemoglobin;
    scanf("%d%d%d",&age,&weight,&hemoglobin);
    if(age>=18 && weight>=45 && hemoglobin>=5.5)
    {
        printf("He or She is unable to donate blood");
    }
    else
    {
        printf("He or She unable to donate blood");
        
    }
   return 0;
}
